{
  "name": "ethcomputationalorgs",
  "version": "1.0.0",
  "description": "Repo containing the newly updated **dfo-protocol** contracts.",
  "main": "build-contracts.js",
  "scripts": {
    "postinstall": "cross-var solc-vm install $npm_package_config_solidityVersion",
    "flat": "node util/contracts-flattener.js",
    "compile": "node util/contracts-compiler.js",
    "abi": "node util/contracts-compiler.js nobin",
    "build": "yarn flat && yarn compile",
    "test": "mocha",
    "server": "node util/server.js",
    "cli-compiler": "node util/cli-compiler.js ext/delegationsManager/impl/DelegationsManager"
  },
  "config": {
    "solidityVersion": "0.8.6"
  },
  "mocha": {
    "timeout": false,
    "require": "util/mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/b-u-i-d-l/ETHComputationalOrgs.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/b-u-i-d-l/ETHComputationalOrgs/issues"
  },
  "homepage": "https://github.com/b-u-i-d-l/ETHComputationalOrgs#readme",
  "dependencies": {
    "@ethereansos/covenants": "git+https://github.com/EthereansOS/Covenants-Core.git#farm_min_stake",
    "@ethereansos/items-v2": "git+https://github.com/EthereansOS/ITEMS-Core.git#v2-projections",
    "@ethereumjs/tx": "^3.4.0",
    "@openzeppelin/contracts": "4.2.0",
    "@solidity-parser/parser": "^0.14.0",
    "cross-var": "^1.1.0",
    "dotenv": "^8.2.0",
    "ethers": "^5.4.7",
    "ganache": "^7.0.2",
    "glob": "^7.1.6",
    "keccak": "^3.0.2",
    "memdown": "^6.1.1",
    "mocha": "^8.2.1",
    "node-html-parser": "^4.1.5",
    "solc-vm": "https://gitpkg.now.sh/EthereansOS/Multiverse-Interface/solc-vm?master",
    "truffle-flattener-wrapper": "https://gitpkg.now.sh/EthereansOS/Multiverse-Interface/truffle-flattener-wrapper?master",
    "web3": "^1.3.3"
  }
}
